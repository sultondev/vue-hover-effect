.container {
    position: relative;
    height: v-bind(height);
    width: v-bind(width);
    min-height: 100%;
    img {
        width: inherit;
        height: inherit;
        position: absolute;
        left: 0;
        top: 0;
        z-index: -1;
        min-height: 200px;
        min-width: 200px;
        max-width: 100%;
    }
}